<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="client.scenes.AdminEventInfoCtrl">
    <children>
        <Label fx:id="titleLabel" layoutX="59.0" layoutY="50.0" prefHeight="66.0" prefWidth="169.0" text="%event_name" />
        <Label fx:id="eventCodeLabel" layoutX="59.0" layoutY="100.0" prefHeight="66.0" prefWidth="169.0" text="%event_code" />
        <Label fx:id="descriptionLabel" layoutX="59.0" layoutY="150.0" prefHeight="66.0" prefWidth="169.0" text="%event_description" />
        <Button fx:id="adminDownloadButton" layoutX="493.0" layoutY="350.0" mnemonicParsing="false" prefHeight="36.0" prefWidth="93.0" text="%download" />
        <Button fx:id="backButton" layoutX="13.0" layoutY="350.0" mnemonicParsing="false" onAction="#backToAdminOverview" prefHeight="36.0" prefWidth="93.0" text="%back" />
        <Button fx:id="adminDeleteButton" layoutX="378.0" layoutY="350.0" mnemonicParsing="false" onAction="#deleteEvent" prefHeight="36.0" prefWidth="93.0" style="-fx-background-color: red;" text="%delete" textFill="WHITE" />
